  <script>
var page = parseInt('<%=page%>')

function next() {
  page+=1;
  document.location.href = "/audit?page=" + page;
}
function prev() {
  if (page - 1 >= 0) {
    page-=1;
    document.location.href = "/audit?page=" + page;
  }
}

function last_online(){
  document.location.href = "/last";
}

function voice(){
